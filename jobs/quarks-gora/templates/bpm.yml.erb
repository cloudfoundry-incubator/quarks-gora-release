processes:
  - name: quarks-gora
    executable: /var/vcap/packages/quarks-gora/bin/quarks-gora
    args: []
    env:
      PORT: "<%= p("quarks-gora.port") %>"
      SERVER_CRT: "/var/vcap/jobs/quarks-gora/config/cert"
      SERVER_KEY: "/var/vcap/jobs/quarks-gora/config/key"
      SSL: "<%= p("quarks-gora.ssl") %>"
      TEXT_MESSAGE: "<%= p("text_message") %>"
